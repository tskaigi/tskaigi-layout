{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "additionalProperties": false,
  "default": {},
  "properties": {
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "hashtag": {
      "type": "object",
      "properties": {
        "globalHashtag": {
          "type": "string"
        },
        "track": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "hashtag": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "dayZero": {
      "type": "object",
      "properties": {
        "year": {
          "type": "integer"
        },
        "month": {
          "type": "integer"
        },
        "day": {
          "type": "integer"
        }
      },
      "additionalProperties": false
    }
  }
}
